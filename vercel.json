{
  "headers": [
    {
      "source": "/(.*).svg",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/svg+xml"
        }
      ]
    },
    {
      "source": "/(.*).gif",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/gif"
        }
      ]
    }
  ]
}